%Read in a concatenated file where it reports A then R in each column.
%So min size is 2 columns for AR. 
function[peakstats]=calc_peak_seps(time, AR)


[len, N]=size(AR);
Ntraj=N/2;

for i=1:2:Ntraj
    [pksA(:,i),locA(:,i)]=findpeaks(AR(:,i),'MinPeakWidth',2);
    [pksR(:,i),locR(:,i)]=findpeaks(AR(:,i+1),'MinPeakWidth',2);
